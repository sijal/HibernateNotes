<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.onetoone.entities">
	<class name="Bill" table="bill">
		<id name="billNo" column="bill_no">
			<generator class="increment" />
		</id>
		<property name="consumerName" column="consumer_nm" />
		<property name="billGeneratedDate" column="bill_generated_dt" />
		<property name="dueDate" column="due_dt" />
		<property name="amount" />
	</class>
</hibernate-mapping>