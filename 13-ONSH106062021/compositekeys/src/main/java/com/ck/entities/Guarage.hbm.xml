<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ck.entities">
	<class name="Guarage" table="guarage">
		<composite-id name="guaragePk">
			<key-property name="guarageClubRegNo"
				column="guarage_club_reg_no" />
			<key-property name="guarageName" column="guarage_nm" />
		</composite-id>
		<property name="proprietorName" column="proprietorName" />
		<property name="contactNo" column="contact_no" />
		<property name="emailAddress" column="email_address" />
		<property name="location" />
	</class>
</hibernate-mapping>