<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.hormex2.entities">
	<class name="Branch">
		<id name="branchNo">
			<column name="branch_no" sql-type="int" />
		</id>
		<property name="branchName">
			<column name="branch_nm" sql-type="varchar" length="45"
				not-null="true" />
		</property>
		<property name="bankName">
			<column name="bank_nm" sql-type="varchar" length="45"
				not-null="true" />
		</property>
		<property name="ifscCode">
			<column name="ifsc_code" sql-type="varchar" length="15"
				not-null="true" />
		</property>
		<property name="location">
			<column name="location" sql-type="varchar" length="45"
				not-null="true" />
		</property>
	</class>
</hibernate-mapping>