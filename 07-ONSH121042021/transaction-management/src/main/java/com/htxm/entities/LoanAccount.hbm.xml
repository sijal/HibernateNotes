<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.htxm.entities">
	<class name="LoanAccount" table="loan_account">
		<id name="loanAccountNo" column="loan_account_no" />
		<property name="loanAccountHolderName"
			column="loan_account_holder_nm" />
		<property name="loanType" column="loan_type" />
		<property name="tenure" />
		<property name="amount" />
	</class>
</hibernate-mapping>